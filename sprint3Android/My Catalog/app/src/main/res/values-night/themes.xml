<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Base.Theme.MyCatalog" parent="Theme.Material3.DayNight.NoActionBar">
    </style>

    <style name="AppTheme" parent="AppTheme" tools:ignore="ResourceCycle">
        <item name="colorPrimary">@color/night_primary</item>
        <item name="colorPrimaryDark">@color/night_primaryDark</item>
        <item name="colorAccent">@color/night_accent</item>
