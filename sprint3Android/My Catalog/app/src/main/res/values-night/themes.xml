    <color name="night_primary" tools:ignore="MissingDefaultResource">#007ACC</color>
    <color name="night_primaryDark" tools:ignore="MissingDefaultResource">#111111</color>
    <color name="night_accent" tools:ignore="MissingDefaultResource">#E0E0E0</color>

</resources>